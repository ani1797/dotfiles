#!/usr/bin/env bash
#
# log_success: Print an success message
#
# Usage: log_success <message>
# Example: log_success "Some success message"
# Prints: [SUCCESS] Some success message
# [SUCCESS] in green color and bold text.
#

log_success() {
    message=$1
    echo -e "\033[1;32m[SUCCESS]\033[0m $message"
}

log_success "$@"